{
  "name": "@tinkerer-monorepo/source",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "version": "0.0.0",
  "scripts": {
    "lint:affected": "nx affected -t lint --parallel=3",
    "test:affected": "nx affected -t test --parallel=3",
    "build:affected": "nx affected -t build --parallel=3",
    "build:all": "yarn nx run-many -t build --parallel=3",
    "all": "yarn nx run-many -t build lint test",
    "format": "yarn prettier --write .",
    "clean-deps": "rm -rf ./apps/*/node_modules & rm -rf ./node_modules",
    "clean-dist": "rm -rf ./apps/*/dist & rm -rf ./dist"
