{
  "name": "@tinkerer-monorepo/source",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "version": "0.0.0",
  "scripts": {
    "serve": "ladle serve -p 61110",
    "serve-prod": "ladle preview -p 61110",
    "build-preview": "ladle build && ladle preview -p 61110",
    "build": "ladle build",
    "test-dev": "yarn exec playwright test",
    "test-prod": "NODE_ENV=production yarn exec playwright test",
    "test": "yarn test-dev && yarn test-prod",
    "test:update": "yarn exec playwright test -u",
    "lint:affected": "nx affected -t lint --parallel=3",
    "test:affected": "nx affected -t test --parallel=3",
    "build:affected": "nx affected -t build --parallel=3",
    "build:all": "yarn nx run-many -t build --parallel=3",
    "all": "yarn nx run-many -t build lint test",
    "format": "yarn prettier --write .",
    "check": "yarn biome check .",
    "write": "yarn biome format . --write",
    "lint": "yarn biome lint . --apply",
    "clean-cache": "yarn cache clean & rm -rf ./.nx/cache",
    "clean-deps": "rm -rf ./apps/*/node_modules & rm -rf ./node_modules",
    "clean-dist": "rm -rf ./apps/*/dist & rm -rf ./dist"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
